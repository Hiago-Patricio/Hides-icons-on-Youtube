{
    "name": "Hides Title and Buttons on Youtube Video",
    "manifest_version": 3,
    "version": "0.1",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": "Ctrl+Shift+L",
            "description": "Toggle visibility"
        }
    },
    "action": {},
    "background": {
        "service_worker": "background.js"
    }
}
